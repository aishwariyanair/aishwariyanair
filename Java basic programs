public class Swap {
    public static void main(String args[]) {
      int a =32; int b=42;
      System.out.println("Before swapping");
      System.out.println("First number is "+a);
      System.out.println("Second number is "+b);
       int temp=a;
       a=b;
       b=temp;
       System.out.println("After swapping");
       System.out.println("First number is "+ a);
       System.out.println("Second number is "+b);
    }}
